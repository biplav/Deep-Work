{
    "name": "Deep Work",
    "description": "Go to focussed mode in chrome, where only single active tab is always maintained",
    "version": "0.1.1",
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "permissions": [
        "tabs",
        "storage"
    ],
    "browser_action": {
        "default_icon": {
            "38": "icons/disabled_38.png"
        },
        "default_title": "Focus Mode"
    },
    "icons": {
        "200": "icons/enabled.png"
    },
    "manifest_version": 2
}